{
  "directories": {
    "test": "test"
  },
  "author": "azu",
  "license": "MIT",
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "name": "faao",
  "description": "Faao is a GitHub issue client like jasper.",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "precommit": "lint-staged",
    "postcommit": "git reset",
    "test": "jest",
    "lint:depcruise": "depcruise --validate .dependency-cruiser.json src",
    "prepublishOnly": "npm run --if-present build",
    "prettier": "prettier \"src/**/*.{ts,tsx,css}\" --write --print-width 100 --tab-width 4",
    "start": "npm-run-all clean --parallel watch:css server",
    "start:electron": "npm-run-all start electron",
    "electron": "cross-env FAAO_URL=http://localhost:8080/ electron electron.js",
    "server": "cross-env NODE_ENV=development RUNTIME_TARGET=browser webpack-dev-server --hot --content-base ./public",
    "build:browser": "cross-env NODE_ENV=production npm-run-all clean --parallel build:browser:* build:css",
    "build:electron": "cross-env NODE_ENV=production npm-run-all clean --parallel build:electron:* build:css",
    "build:browser:js": "cross-env NODE_ENV=production RUNTIME_TARGET=browser webpack --progress -p",
    "build:electron:js": "cross-env NODE_ENV=production RUNTIME_TARGET=electron webpack --progress -p",
    "build:css": "postcss --config postcss.config.js src/index.css -o public/build/build.css",
    "build:docs": "cross-env NODE_ENV=production webpack --config ./webpack.docs.config.js -p",
    "watch:browser:js": "cross-env NODE_ENV=development RUNTIME_TARGET=browser webpack --watch",
    "watch:electron:js": "cross-env NODE_ENV=development RUNTIME_TARGET=electron webpack --watch",
    "watch:css": "chokidar --initial 'src/**/*.css' -c 'cross-env NODE_ENV=development npm run build:css'",
    "watch:browser": "npm-run-all clean --parallel watch:browser:* watch:css",
    "watch:electron": "npm-run-all clean --parallel watch:electron:* watch:css",
    "clean": "rimraf public/build && mkdirp public/build",
    "ci": "npm test && cross-env NODE_ENV=development RUNTIME_TARGET=browser webpack"
  },
  "devDependencies": {
    "@alrra/travis-scripts": "^3.0.1",
    "@types/classnames": "^2.2.0",
    "@types/electron": "^1.6.10",
    "@types/glob": "^5.0.30",
    "@types/jest": "^19.2.4",
    "@types/lodash.groupby": "^4.6.2",
    "@types/node": "^7.0.31",
    "autoprefixer": "^7.1.1",
    "awesome-typescript-loader": "^3.1.3",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chokidar-cli": "^1.2.0",
    "cross-env": "^5.0.1",
    "cssnano": "^3.10.0",
    "dependency-cruiser": "^2.2.3",
    "electron": "1.7.2",
    "glob": "^7.1.2",
    "husky": "^0.13.4",
    "jest": "^20.0.4",
    "json-loader": "^0.5.4",
    "lint-staged": "^3.6.1",
    "lodash.groupby": "^4.6.0",
    "minimatch": "^3.0.4",
    "mkdirp": "^0.5.1",
    "nomnoml": "^0.1.1",
    "npm-run-all": "^4.0.2",
    "postcss-calc": "^6.0.0",
    "postcss-cli": "^4.1.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^6.0.1",
    "postcss-easy-import": "^2.1.0",
    "postcss-normalize": "^4.0.0",
    "postcss-reporter": "^4.0.0",
    "prettier": "^1.4.4",
    "rimraf": "^2.6.1",
    "ts-jest": "^20.0.6",
    "typemoq": "^1.7.0",
    "typescript": "^2.3.4",
    "webpack": "^2.6.1",
    "webpack-build-dll-plugin": "^1.3.2",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "@immutable-array/prototype": "^1.0.2",
    "@types/combokeys": "^2.4.5",
    "@types/lodash.sortby": "^4.7.2",
    "@types/lodash.throttle": "^4.1.2",
    "@types/lodash.uniqby": "^4.7.2",
    "@types/moment": "^2.13.0",
    "@types/prop-types": "^15.5.1",
    "@types/react": "^15.0.29",
    "@types/react-color": "^2.11.2",
    "@types/react-dom": "^15.5.0",
    "@types/url-join": "^0.8.2",
    "almin": "0.13.3",
    "almin-logger": "^4.0.3",
    "almin-react-container": "0.3.4",
    "classnames": "^2.2.5",
    "codemirror": "^5.27.2",
    "combokeys": "^2.4.6",
    "debug": "^2.6.8",
    "electron-context-menu": "^0.9.1",
    "electron-default-menu": "^1.0.1",
    "electron-navigation": "^1.5.3",
    "execall": "^1.0.0",
    "filterable": "^2.0.1",
    "github-api": "^3.0.0",
    "github-url-to-object": "^4.0.2",
    "is-electron": "^2.0.0",
    "localforage": "^1.5.0",
    "localforage-memoryStorageDriver": "^0.9.2",
    "lodash.sortby": "^4.7.0",
    "lodash.throttle": "^4.1.1",
    "lodash.uniqby": "^4.7.0",
    "lru-map-like": "^2.0.0",
    "map-like": "1.1.3",
    "moment": "^2.18.1",
    "octokat": "^0.8",
    "office-ui-fabric-react": "~4.6.0",
    "parse-github-event": "^1.0.2",
    "react": "^15.6.1",
    "react-codemirror": "git://github.com/skidding/react-codemirror.git#106-fix-update",
    "react-color": "^2.13.0",
    "react-dom": "^15.6.1",
    "react-octicons": "^0.1.0",
    "react-topbar-progress-indicator": "^1.0.0",
    "request-idle-polyfill": "^1.0.1",
    "shallow-equal-object": "^1.0.1",
    "suitcss-classnames": "^1.1.0",
    "ulid": "^0.2.0",
    "url-join": "^2.0.2",
    "whatwg-fetch": "^2.0.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "node_modules",
      "lib"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,css}": [
      "prettier --write --print-width 100 --tab-width 4",
      "git add"
    ]
  }
}
