  "author": "azu",
  "license": "MIT",
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "name": "faao",
  "description": "Faao is a GitHub issue client like jasper.",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "mocha test/",
    "prepublish": "npm run --if-present build",
    "start": "npm-run-all clean --parallel watch:css server",
    "server": "cross-env NODE_ENV=development webpack-dev-server --hot --content-base ./public --open",
    "build:js": "cross-env NODE_ENV=production webpack --progress -p",
    "build:css": "postcss --config postcss.config.js src/index.css -o public/build/build.css",
    "build": "npm-run-all clean --parallel build:*",
    "watch:js": "cross-env NODE_ENV=development webpack --watch",
    "watch:css": "chokidar --initial 'src/**/*.css' -c 'npm run build:css'",
    "watch": "npm-run-all clean --parallel watch:*",
    "clean": "rimraf public/build && mkdirp public/build"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.23",
    "autoprefixer": "^7.1.1",
    "awesome-typescript-loader": "^3.1.3",
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-jsdoc-to-assert": "^4.0.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "chokidar-cli": "^1.2.0",
    "cross-env": "^5.0.0",
    "espower-typescript": "^8.0.0",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "mocha": "^3.4.2",
    "npm-run-all": "^4.0.2",
    "postcss-calc": "^6.0.0",
    "postcss-cli": "^4.0.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^6.0.1",
    "postcss-easy-import": "^2.0.0",
    "postcss-reporter": "^3.0.0",
    "power-assert": "^1.4.2",
    "rimraf": "^2.6.1",
    "typescript": "^2.3.4",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-dotenv-plugin": "^2.0.0"
  },
  "dependencies": {
    "@types/prop-types": "^15.5.1",
    "@types/react": "^15.0.25",
    "@types/react-dom": "^15.5.0",
    "almin": "0.13.0",
    "almin-logger": "^4.0.3",
    "almin-react-container": "0.3.0",
    "classnames": "^2.2.5",
    "debug": "^2.6.8",
    "github-api": "^3.0.0",
    "map-like": "^1.1.2",
    "octokat": "^0.8.0",
    "office-ui-fabric-react": "^4.1.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "suitcss-classnames": "^1.1.0"
  }
}
